<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="log.bewell.login">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
        android:noHistory = "true"
            >
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DashBoard"
            android:parentActivityName=".HomeScreen"
            android:screenOrientation="portrait"/>
        <activity android:name=".ForgotPassword"  android:screenOrientation="portrait" />
        <activity android:name=".SignUp"  android:screenOrientation="portrait" />
        <activity android:name=".HomeScreen"  android:screenOrientation="portrait"/>
        <activity android:name=".nutritionrecorder.CalorieActivity"  android:screenOrientation="portrait"/>
        <activity android:name=".nutritionrecorder.WebActivity"  android:screenOrientation="portrait" />
        <activity android:name=".SupportActivity"  android:screenOrientation="portrait"/>
        <activity
            android:name=".FHomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProgressActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LevelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WorkoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CongratulationsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InstructionsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CreditsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".nutritionrecorder.InputFood"
            android:label="@string/title_activity_input_food"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".nutritionrecorder.Statistics"
            android:label="@string/title_activity_statistics"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".nutritionrecorder.UserInfoCal"
            android:label="User information for TDEE"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>


        <activity android:name=".RelaxActivity"  android:screenOrientation="portrait"
            android:label="Relaxation"/>
        <activity android:name=".PushUpVideoActivity"  android:screenOrientation="portrait"/>
        <activity android:name=".SitUpVideoActivity"  android:screenOrientation="portrait" />
        <activity android:name=".SquatVideoActivity"  android:screenOrientation="portrait" />
        <activity android:name=".Contact_InfoActivity"
            android:label="Support Page"  android:screenOrientation="portrait"></activity>


        //Quiz
        <activity android:name="log.bewell.login.quiz.QuizActivity"  android:screenOrientation="portrait"></activity>
        <activity android:name="log.bewell.login.quiz.QuizStartActivity"  android:screenOrientation="portrait" ></activity>

    </application>

</manifest>